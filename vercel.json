{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": { 
          "runtime": "python3.12.8",
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }
  